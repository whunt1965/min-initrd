#!/bin/bash
mount -t proc /proc /proc
mount -t sysfs /sys /sys


rm -rf mytmpfs
mkdir /mytempfs
mount -t tmpfs -o size=4g tmpfs /mytmpfs

echo Welcome to supermin

#Set User for memcached
echo "ukl_user:x:0:0:root:/ukl_user:/bin/sh" > etc/passwd
echo "ukl_user::10933:0:99999:7:::" > etc/shadow
echo "ukl_user:x:0:" > etc/group

bash -i

